n      const tasksFromServer = await fetchTasks();\n      dispatch({\n        tasksFromServer,\n        type: 'FETCH_TASKS',\n      });\n    };\n    getTasks();\n  }, [globalState, tasks, showAddTask]);\n\n  // Add Task\n  const addTask = async (task) => {\n    const res = await fetch(TASK_ENDPOINT, {\n      method: POST_HTTP_METHOD,\n      headers: {\n        'Content-type': 'application/json',\n      },\n      body: JSON.stringify(task),\n    });\n\n    const data = await res.json()\n    .then(data => dispatch({type: 'ADD_TASK', data}))\n\n  };\n\n  // Delete Task\n  const deleteTask = async (id) => {\n    const res = await fetch(`${TASK_ENDPOINT}/${id}`, {\n      method: DELETE_HTTP_METHOD,\n    });\n    // We should control the response status to decide if we will change the state or not.\n    res.status === 200\n      ? dispatch({ action: 'DELETE_TASK', id })\n      : alert('Error Deleting This Task');\n  };\n\n  // Toggle Reminder - come back to this\n  const toggleReminder = async (id) => {\n    const taskToToggle = await fetchTask(id);\n    const updTask = { ...taskToToggle, reminder: !taskToToggle.reminder };\n\n    const res = await fetch(`http://localhost:5000/tasks/${id}`, {\n      method: PUT_HTTP_METHOD,\n      headers: {\n        'Content-type': 'application/json',\n      },\n      body: JSON.stringify(updTask),\n    });\n\n    const data = await res.json();\n  };\n\n  return (\n    \n    <StoreContext.Provider value={[globalState, dispatch]}>\n      <Router>\n        <div className=\"container\">\n          <Header />\n          <Route\n            path=\"/\"\n            exact\n            render={(props) => (\n              <>\n                {showAddTask && <AddTask onAdd={addTask} />}\n                {tasks.length > 0 ? (\n                  <Tasks\n                    tasks={tasks}\n                    onDelete={deleteTask}\n                    onToggle={toggleReminder}\n                  />\n                ) : (\n                  'No Tasks To Show'\n                )}\n              </>\n            )}\n          />\n          <Route path=\"/about\" component={About} />\n          <Footer />\n        </div>\n      </Router>\n    </StoreContext.Provider>\n  );\n};\n\nexport default App;\n","/Users/jpboehman/Algorithm/JavaScript_Algorithms/react-crashcourse/react-task-tracker/src/reportWebVitals.js",[],"/Users/jpboehman/Algorithm/JavaScript_Algorithms/react-crashcourse/react-task-tracker/src/components/Footer.js",[],"/Users/jpboehman/Algorithm/JavaScript_Algorithms/react-crashcourse/react-task-tracker/src/components/Header.js",[],"/Users/jpboehman/Algorithm/JavaScript_Algorithms/react-crashcourse/react-task-tracker/src/components/Tasks.js",[],"/Users/jpboehman/Algorithm/JavaScript_Algorithms/react-crashcourse/react-task-tracker/src/components/AddTask.js",[],"/Users/jpboehman/Algorithm/JavaScript_Algorithms/react-crashcourse/react-task-tracker/src/components/About.js",[],"/Users/jpboehman/Algorithm/JavaScript_Algorithms/react-crashcourse/react-task-tracker/src/components/Button.js",["64"],"/Users/jpboehman/Algorithm/JavaScript_Algorithms/react-crashcourse/react-task-tracker/src/components/Task.js",[],"/Users/jpboehman/Algorithm/JavaScript_Algorithms/react-crashcourse/react-task-tracker/src/apiCalls/fetchData.js",[],"/Users/jpboehman/Algorithm/JavaScript_Algorithms/react-crashcourse/react-task-tracker/src/constants/constants.js",[],"/Users/jpboehman/Algorithm/JavaScript_Algorithms/react-crashcourse/react-task-tracker/src/Store/reducer.js",[],"/Users/jpboehman/Algorithm/JavaScript_Algorithms/react-crashcourse/react-task-tracker/src/Store/index.js",[],{"ruleId":"65","replacedBy":"66"},{"ruleId":"67","replacedBy":"68"},{"ruleId":"69","severity":1,"message":"70","line":48,"column":11,"nodeType":"71","messageId":"72","endLine":48,"endColumn":15},{"ruleId":"69","severity":1,"message":"70","line":77,"column":11,"nodeType":"71","messageId":"72","endLine":77,"endColumn":15},{"ruleId":"69","severity":1,"message":"73